        <rdfs:range rdf:resource="http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#Information"/>
        <rdfs:comment>relates an individual to information they come across.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#hasBeliefSystem -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#hasBeliefSystem">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#Individual"/>
        <rdfs:range rdf:resource="https://w3id.org/framester/conceptnet/5.7.0/c/en/belief_system"/>
        <rdfs:comment>relates an individual to their set of pre-existing beliefs.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#hasFilteringMechanism -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#hasFilteringMechanism">
