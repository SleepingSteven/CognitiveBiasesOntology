    <!-- http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#confirms -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#confirms">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#Information"/>
        <rdfs:range rdf:resource="https://w3id.org/framester/conceptnet/5.7.0/c/en/belief_system"/>
        <rdfs:comment>relates an instance of information to the belief system it supports, indicating confirmation bias.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#encounters -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#encounters">
