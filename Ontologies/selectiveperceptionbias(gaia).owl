    <!-- http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#Information -->

    <owl:Class rdf:about="http://www.semanticweb.org/hp/ontologies/2024/gaia/selectiveperception/#Information">
        <rdfs:comment>represents data, facts, or stimuli that individuals encounter.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/framester/conceptnet/5.7.0/c/en/belief_system -->

    <owl:Class rdf:about="https://w3id.org/framester/conceptnet/5.7.0/c/en/belief_system">
        <rdfs:comment>represents the set of pre-existing beliefs, attitudes, or expectations an individual holds.</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/framester/conceptnet/5.7.0/c/en/filtering_mechanism -->

    <owl:Class rdf:about="https://w3id.org/framester/conceptnet/5.7.0/c/en/filtering_mechanism">
        <rdfs:comment>represents the cognitive process through which individuals selectively attend to certain information and filter out others based on their biases.</rdfs:comment>
    </owl:Class>
    


