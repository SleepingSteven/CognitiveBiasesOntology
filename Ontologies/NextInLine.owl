        <rdfs:domain rdf:resource="http://www.semanticweb.org/lucia/ontologies/2024/0/next-in-line#Turn"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lucia/ontologies/2024/0/next-in-line#Participant"/>
    </owl:ObjectProperty>
    


