    <!-- http://www.semanticweb.org/lucia/ontologies/2024/0/next-in-line#hasParticipant -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/lucia/ontologies/2024/0/next-in-line#hasParticipant">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


