<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.matteoguenci.it/CognitiveBiasOntology/SemmelweisReflex.owl#"
     xml:base="http://www.matteoguenci.it/CognitiveBiasOntology/SemmelweisReflex.owl"
     xmlns:dc="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://www.matteoguenci.it/CognitiveBiasOntology/SemmelweisReflex.owl">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl"/>
        <dc:title xml:lang="en">Semmelweis Reflex Ontology</dc:title>
        <rdfs:comment xml:lang="en">An ontology representing the Semmelweis Reflex as a specific cognitive bias.</rdfs:comment>
    </owl:Ontology>

    <!-- Classes -->
    <owl:Class rdf:about="#CognitiveBias">
        <rdfs:comment xml:lang="en">Parent class for all cognitive biases.</rdfs:comment>
        <rdfs:label xml:lang="en">CognitiveBias</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#SemmelweisReflex">
        <rdfs:subClassOf rdf:resource="#CognitiveBias"/>
        <rdfs:comment xml:lang="en">Represents the specific cognitive bias of rejecting new evidence or knowledge because it contradicts established norms, beliefs, or paradigms.</rdfs:comment>
        <rdfs:label xml:lang="en">SemmelweisReflex</rdfs:label>
    </owl:Class>

    <!-- Properties -->
    <owl:ObjectProperty rdf:about="#hasDefinition">
        <rdfs:domain rdf:resource="#CognitiveBias"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">hasDefinition</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasExample">
        <rdfs:domain rdf:resource="#CognitiveBias"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">hasExample</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#affectsDecisionMaking">
        <rdfs:domain rdf:resource="#CognitiveBias"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">affectsDecisionMaking</rdfs:label>
    </owl:ObjectProperty>

    <!-- Instances -->
    <owl:NamedIndividual rdf:about="#SemmelweisReflexInstance">
        <rdf:type rdf:resource="#SemmelweisReflex"/>
        <rdf:type rdf:resource="#CognitiveBias"/>
        <rdfs:label xml:lang="en">Semmelweis Reflex Instance</rdfs:label>
        <rdfs:comment xml:lang="en">Meet Dr. Smith, a seasoned medical practitioner. Dr. Smith has always believed in a certain treatment method for a particular disease based on his years of experience. When a new research study comes out suggesting a more effective treatment method, Dr. Smith immediately dismisses it, believing that his tried-and-true method is superior. This is an example of the Semmelweis Reflex, as Dr. Smith is rejecting new knowledge that contradicts his established beliefs.</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Relationships -->
    <rdf:Description rdf:about="#SemmelweisReflexInstance">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
        <owl:onProperty rdf:resource="#isA"/>
        <owl:allValuesFrom rdf:resource="#SemmelweisReflex"/>
    </rdf:Description>

    <rdf:Description rdf:about="#SemmelweisReflex">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
        <owl:onProperty rdf:resource="#isA"/>
        <owl:allValuesFrom rdf:resource="#CognitiveBias"/>
        <rdfs:comment xml:lang="en">The Semmelweis Reflex or 'Semmelweis Effect' is a metaphor for the reflex-like tendency to reject new evidence or new knowledge because it contradicts established norms, beliefs, or paradigms.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="#SemmelweisReflexInstance">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
        <owl:onProperty rdf:resource="#hasExample"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Dr. Smith, a seasoned medical practitioner, immediately dismisses a new research study suggesting a more effective treatment method, believing that his tried-and-true method is superior.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="#SemmelweisReflex">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
        <owl:onProperty rdf:resource="#hasDefinition"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The Semmelweis Reflex or 'Semmelweis Effect' is a metaphor for the reflex-like tendency to reject new evidence or new knowledge because it contradicts established norms, beliefs, or paradigms.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="#SemmelweisReflex">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
        <owl:onProperty rdf:resource="#affectsDecisionMaking"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The Semmelweis Reflex can lead individuals to reject new ideas or discoveries that contradict their existing beliefs, affecting their decision-making process.</rdfs:comment>
    </rdf:Description>

</rdf:RDF>
