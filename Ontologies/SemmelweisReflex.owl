    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isAware -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isAware">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialAgent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the entity is aware of their state.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isChronic -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isChronic">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the state is chronic or long-lasting.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isComplex -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isComplex">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontology.se/odp/content/owl/Action.owl#Action"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#Environment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the environment, or the action to perform, is complex or challenging.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isNegative -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isNegative">
        <rdfs:domain rdf:resource="http://descartes-core.org/ontologies/activities/1.0/ActivityPattern.owl#Outcome"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the consequence is negative or harmful.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isStrong -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isStrong">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#Perception"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates the strength or intensity of the perception.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isVerified -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#isVerified">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/semmelweis_reflex#Fact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the fact is verified or confirmed.</rdfs:comment>
    </owl:DatatypeProperty>
    


