    <!-- http://etna.istc.cnr.it/framester2/conceptnet/5.7.0/c/en/challenges -->

    <owl:ObjectProperty rdf:about="http://etna.istc.cnr.it/framester2/conceptnet/5.7.0/c/en/challenges">
        <owl:inverseOf rdf:resource="http://etna.istc.cnr.it/framester2/conceptnet/5.7.0/c/en/confirms"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#Challenge"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#Belief"/>
        <rdfs:comment>Links the challenge to the belief it challenges.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://etna.istc.cnr.it/framester2/conceptnet/5.7.0/c/en/confirms -->

    <owl:ObjectProperty rdf:about="http://etna.istc.cnr.it/framester2/conceptnet/5.7.0/c/en/confirms">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#Confirmation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#Belief"/>
        <rdfs:comment>Links the confirmation to the belief it supports.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://krisnadhi.github.io/onto/event.owl#hasInformationObject -->

    <owl:ObjectProperty rdf:about="http://krisnadhi.github.io/onto/event.owl#hasInformationObject">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialAgent"/>
        <rdfs:range rdf:resource="http://krisnadhi.github.io/onto/event.owl#InformationObject"/>
        <rdfs:comment>Links the actor to the information they encounter.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ontology.se/odp/content/owl/Action.owl#has_consequence -->

    <owl:ObjectProperty rdf:about="http://www.ontology.se/odp/content/owl/Action.owl#has_consequence">
        <rdfs:domain rdf:resource="http://www.ontology.se/odp/content/owl/Action.owl#Action"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#BehavioralOutcome"/>
        <rdfs:comment>(owl:TransitiveProperty) A causal relation between actions, i.e. one action is the cause of another action. For example, the action of &quot;swimming&quot; is a consequence of &quot;jumping into deep water&quot;. The property is transitive.</rdfs:comment>
    </owl:ObjectProperty>
    


