        <rdfs:range rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#Belief"/>
        <rdfs:comment>Links the social agent to the belief they hold.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#influences -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#influences">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#Belief"/>
        <rdfs:range rdf:resource="http://www.ontology.se/odp/content/owl/Action.owl#Action"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#interpret -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#interpret">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialAgent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#Statement"/>
        <rdfs:comment>Links the actor to the statement they interpret.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#providesInformation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#providesInformation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#Statement"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#Belief"/>
        <rdfs:comment>Relates an Information to a Belief. The information that can be ambiguous strongly influences the beliefe</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#takesAction -->

