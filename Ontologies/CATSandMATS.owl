                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/1/CognitiveBiasOntology/continued_influence_effect#originatesFrom"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/1/CognitiveBiasOntology/continued_influence_effect#Source"/>
