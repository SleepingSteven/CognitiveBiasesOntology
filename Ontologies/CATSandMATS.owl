    <!-- http://etna.istc.cnr.it/framester2/data/framestercore/Information -->

    <owl:Class rdf:about="http://etna.istc.cnr.it/framester2/data/framestercore/Information">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
