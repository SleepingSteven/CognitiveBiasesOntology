        <rdfs:range rdf:resource="http://descartes-core.org/ontologies/activities/1.0/ActivityPattern.owl#Outcome"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/subjective_validation#BehavioralOutcome"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
