    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/ostrich_effect#AcademicSetting -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/ostrich_effect#AcademicSetting">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation"/>
        <Action:has_consequence rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/ostrich_effect#FinallyCheckingGrades"/>
        <Action:has_consequence rdf:resource="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/ostrich_effect#NotCheckingGrades"/>
        <isStressful rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isStressful>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/ostrich_effect#Alex -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/mguen/ontologies/2024/0/CognitiveBiasOntology/ostrich_effect#Alex">
